<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Usuarios</title>
    <link rel="stylesheet" href="css/estilosTabla.css">
</head>
<body>
    <h1>Usuarios</h1>
    <table>
    <thead>
     <tr>
            <th>id</th>
            <th>Nombre</th>
            <th>Correo</th>
            <th>Numero</th>
            <th>Editar / Borrar</th>
        </tr>
    </thead>
    <tbody>
    <% 
    usuariosCorrectos.forEach(usuario => {
    %>
    <tr>
        <td><%= usuario.idusuarios %></td>
        <td><%= usuario.nombre %></td>
        <td><%= usuario.correo %></td>
        <td><%= usuario.celular %></td>
        <td>
            <a href="/editarUsuario/<%= usuario.idusuarios %>">Editar</a>/
            <a href="/borrarUsuario/<%= usuario.idusuarios %>">Borrar</a>
        </td>
    </tr>
    <% 
    });
    %>
</tbody>

</table>
</body>
</html>